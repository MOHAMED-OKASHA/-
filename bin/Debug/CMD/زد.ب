<root>

<constant const1="_const_01" const2="const_02" /> 

<ar_include> </ar_include>



<ar_initial> 


</ar_initial>

<ar_setup>

</ar_setup>

<ar_loop>
incr(_const_01,_const_02);
</ar_loop>

<ar_extra>
void incr(int pin,int value){
int oldValue=analogRead(pin);
analogWrite(pin, oldValue+value); 
}
</ar_extra>
</root>